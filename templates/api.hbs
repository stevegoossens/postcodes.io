export type FactoryFunction<T> = (configuration?: Configuration, basePath?: string, fetch?: FetchAPI) => T;

{{#each groups}}
export namespace {{className name}}Api {
{{#each operations}}
{{#if (gt (count parameters) 1)}}
	{{>frag/apiParametersInterface}}
