	{{else}}
	{{identifier name}}({{#each parameters}}{{>frag/parameter}}, {{/each}}{{#if requestBody.nativeType}}{{#with requestBody}}{{>frag/parameter}}, {{/with}}{{/if}}options?: RequestInit): Promise<{{{className ../name}}}Api.{{{className name}}}Response>
	{{/if}}

{{/each}}
