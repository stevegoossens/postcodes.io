{
  "name": "postcodes.io-client",
  "version": "1.0.0",
  "description": "Client for the postcodes.io API (ideal-postcodes/postcodes.io)",
  "repository": "git@github.com:stevegoossens/postcodes.io-client",
  "author": "stevegoossens <steve.goossens@gmail.com>",
  "license": "MIT",
  "private": false,
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "docs": "redocly preview-docs postcodes.io",
    "pregenerate": "rimraf src/openapi",
    "generate": "openapi-generator-plus generate -c ./config.yml",
    "openapi:lint": "redocly lint postcodes.io",
    "toc:update": "markdown-toc -i README.md",
    "test": "jest",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "yarn run compile",
    "pretest": "yarn run compile",
    "posttest": "yarn run lint"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@jest/globals": "^29.6.1",
    "@openapi-generator-plus/typescript-fetch-node-client-generator": "^1.1.6",
    "@redocly/cli": "^1.0.0-beta.130",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.4.1",
    "@types/node-fetch": "2.x",
    "core-js": "^3.1.4",
    "gts": "^3.1.1",
    "jest": "^29.6.1",
    "markdown-toc": "^1.2.0",
    "mobx": "^6.0.4",
    "nock": "^13.3.1",
    "openapi-generator-plus": "^2.6.0",
    "openapi-types": ">=7",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-is": ">=16.8.0",
    "rimraf": "^5.0.1",
    "styled-components": "^5.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "webpack": "^5.0.0"
  },
  "dependencies": {
    "abab": "^2.0.6",
    "form-data": "^4.0.0",
    "node-fetch": "2.6.7"
  }
}
